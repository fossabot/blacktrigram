{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "name": "Black Trigram (흑괘) - Korean Martial Arts Combat Simulator API",
  "readme": "README.md",
  "theme": "default",
  "plugin": ["typedoc-plugin-mermaid"],
  "exclude": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/.git/**",
    "src/vite-env.d.ts",
    "src/test-types.ts",
    "src/test/**",
    "**/*.ipynb",
    "**/*.css"
  ],
  "excludeExternals": false,
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeNotDocumented": false,
  "hideGenerator": false,
  "includeVersion": true,
  "categorizeByGroup": true,
  "defaultCategory": "General",
  "categoryOrder": [
    "Core",
    "Game Systems",
    "Combat System",
    "Trigram System",
    "Vital Point System",
    "Player & Archetypes",
    "UI Components",
    "Game Components",
    "Audio System",
    "Hooks",
    "Utilities",
    "Type Definitions",
    "Enumerations",
    "Constants",
    "General",
    "*"
  ],
  "sort": ["kind", "instance-first", "alphabetical"],
  "tsconfig": "tsconfig.json",
  "lang": "en",
  "githubPages": true,
  "sourceLinkTemplate": "https://github.com/YOUR_USERNAME/blacktrigram/blob/{gitRevision}/{path}#L{line}",
  "disableSources": false,
  "gitRevision": "main",
  "gitRemote": "origin",
  "navigationLinks": {
    "GitHub": "https://github.com/YOUR_USERNAME/blacktrigram",
    "README": "https://github.com/YOUR_USERNAME/blacktrigram/blob/main/README.md",
    "Game Design": "https://github.com/YOUR_USERNAME/blacktrigram/blob/main/game-design.md",
    "Architecture": "https://github.com/YOUR_USERNAME/blacktrigram/blob/main/ARCHITECTURE.md",
    "Combat System": "https://github.com/YOUR_USERNAME/blacktrigram/blob/main/COMBAT_ARCHITECTURE.md"
  },
  "sidebarLinks": {
    "Game Documentation": "https://github.com/YOUR_USERNAME/blacktrigram/tree/main/docs",
    "Korean Martial Arts": "https://github.com/YOUR_USERNAME/blacktrigram/blob/main/mindmap.md"
  },
  "searchInComments": true,
  "logLevel": "Info",
  "treatWarningsAsErrors": false,
  "pretty": true,
  "groupOrder": [
    "Classes",
    "Interfaces",
    "Type Aliases",
    "Enumerations",
    "Functions",
    "Variables",
    "Namespaces",
    "*"
  ],
  "kindSortOrder": [
    "Module",
    "Namespace",
    "Class",
    "Interface",
    "TypeAlias",
    "Enum",
    "Function",
    "Variable",
    "Property",
    "Method",
    "Constructor",
    "Parameter",
    "TypeParameter",
    "Accessor",
    "GetSignature",
    "SetSignature",
    "IndexSignature",
    "CallSignature",
    "ConstructorSignature"
  ],
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "requiredToBeDocumented": [
    "Module",
    "Namespace",
    "Class",
    "Interface",
    "Property",
    "Method",
    "Function",
    "TypeAlias",
    "Variable",
    "Enum",
    "EnumMember"
  ],
  "visibilityFilters": {
    "protected": true,
    "private": true,
    "inherited": true,
    "external": true
  },
  "commentStyle": "jsdoc",
  "useTsLinkResolution": true,
  "preserveLinkText": false,
  "treatValidationWarningsAsErrors": false,
  "cleanOutputDir": true,
  "titleLink": "/",
  "excludeTags": ["@internal", "@hidden", "@ignore"],
  "externalSymbolLinkMappings": {
    "pixi.js": {
      "Application": "https://pixijs.download/release/docs/app.Application.html",
      "Container": "https://pixijs.download/release/docs/scene.Container.html",
      "Graphics": "https://pixijs.download/release/docs/scene.Graphics.html",
      "Text": "https://pixijs.download/release/docs/scene.Text.html",
      "Sprite": "https://pixijs.download/release/docs/scene.Sprite.html",
      "DisplayObject": "https://pixijs.download/release/docs/scene.DisplayObject.html"
    },
    "react": {
      "*": "https://react.dev/reference/react/"
    },
    "@pixi/react": {
      "*": "https://pixijs.io/pixi-react/"
    }
  },
  "blockTags": [
    "@alpha",
    "@beta",
    "@category",
    "@deprecated",
    "@enum",
    "@event",
    "@example",
    "@experimental",
    "@group",
    "@hidden",
    "@ignore",
    "@inheritDoc",
    "@internal",
    "@label",
    "@link",
    "@module",
    "@namespace",
    "@override",
    "@packageDocumentation",
    "@param",
    "@private",
    "@privateRemarks",
    "@protected",
    "@public",
    "@readonly",
    "@remarks",
    "@returns",
    "@satisfies",
    "@sealed",
    "@see",
    "@since",
    "@static",
    "@template",
    "@throws",
    "@todo",
    "@typeParam",
    "@virtual"
  ],
  "inlineTags": ["@link", "@linkcode", "@linkplain", "@inheritDoc"],
  "modifierTags": [
    "@public",
    "@private",
    "@protected",
    "@internal",
    "@readonly",
    "@override",
    "@virtual",
    "@sealed",
    "@abstract",
    "@static",
    "@experimental",
    "@beta",
    "@alpha",
    "@deprecated"
  ]
}
